{
  "name": "trademark",
  "description": "A end to end automation for trademark filling in Singapore",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "env:set": "bunx dotenvx set",
    "env:get": "bunx dotenvx get",
    "with-env": "dotenvx run --",
    "dev": "bun with-env next dev --port 6969",
    "build": "bun with-env next build",
    "start": "bun with-env next start",
    "clean": "git clean -xdf node_modules .turbo .next .sst",
    "format": "bunx @biomejs/biome format . --write",
    "lint": "bunx @biomejs/biome lint . --write"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.5.2",
    "framer-motion": "^12.0.6",
    "lucide-react": "^0.474.0",
    "next": "15.1.6",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "recharts": "^2.15.1",
    "rehype-pretty-code": "^0.14.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "unified": "^11.0.5",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@dotenvx/dotenvx": "^1.34.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.0.1",
    "typescript": "^5.7.3"
  },
  "packageManager": "bun@1.1.34"
}
